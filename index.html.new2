y = L.tileLayer('https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}', {      attribution: 'Tiles &copy; Esri &mdash; Source: Esri, i-cubed, USDA, USGS, AEX, GeoEye, Getmapping, Aerogrid, IGN, IGP, UPR-EGP, and the GIS User Community'    });    var basemap = {      'Streets': mapboxbasic,      'Aerials': mapboxsat,      'ESRI Imagery': Esri_WorldImagery    } // Refer to the individual Tile Layers only. They will go into the tilePane.    ;    L.control.layers(basemap).addTo(map); // Layers Control just to switch between the 2 basemaps.  </script></body></html>